<?xml version="1.0" encoding="GBK"?>
<!-- 指定Hibernate映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Hibernate映射文件的根元素 -->
<hibernate-mapping package="org.metasee.database.model">
	<class	name="MetaDataSourcecodeOfOriginalPage"	table="sourcecodeOfOriginalPage">
			<!-- 映射标识属性 -->
		<id	name="id" type="integer" column="id">
			<generator class="identity"/>
		</id>
		<!-- 映射普通属性 -->
		<property name="sourcecodeOfOriginalPage" column="sourcecodeOfOriginalPage"	type="string"/>
		
		<!-- 一对一关联 -->
		 <many-to-one name="metaData"
                 class="MetaData"
                 unique="true"
                 column="metaDataid">             
     </many-to-one>
		
	</class>
</hibernate-mapping>